{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "title": "Cheatsheet",
  "type": "object",
  "properties": {
    "metadata": {
      "type": "object",
      "properties": {
        "title": {
          "type": "string"
        },
        "description": {
          "type": "string"
        }
      }
    },
    "payload": {
      "type": "array",
      "properties": {
        "category": {
          "type": "string"
        },
        "terms": {
          "type": "array",
          "properties": {
            "term": {
              "type": "object",
              "properties": {
                "term_name": {
                  "type": "string"
                },
                "term_description": {
                  "type": "string"
                }
              }
            }
          }
        }
      }
    }
  }
}
